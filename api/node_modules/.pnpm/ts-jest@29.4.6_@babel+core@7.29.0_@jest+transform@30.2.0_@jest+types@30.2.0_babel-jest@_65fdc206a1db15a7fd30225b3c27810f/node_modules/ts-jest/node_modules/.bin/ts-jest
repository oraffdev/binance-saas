#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/CODE/binance-trade-bot/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.29.0_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_65fdc206a1db15a7fd30225b3c27810f/node_modules/ts-jest/node_modules:/home/ubuntu/CODE/binance-trade-bot/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.29.0_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_65fdc206a1db15a7fd30225b3c27810f/node_modules:/home/ubuntu/CODE/binance-trade-bot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/CODE/binance-trade-bot/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.29.0_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_65fdc206a1db15a7fd30225b3c27810f/node_modules/ts-jest/node_modules:/home/ubuntu/CODE/binance-trade-bot/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@7.29.0_@jest+transform@30.2.0_@jest+types@30.2.0_babel-jest@_65fdc206a1db15a7fd30225b3c27810f/node_modules:/home/ubuntu/CODE/binance-trade-bot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
